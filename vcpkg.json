{
    "name": "libdigidocpp",
    "dependencies": [
      {
        "name": "libxml2",
        "default-features": false
      },
      "xmlsec",
      "zlib"
    ],
    "features": {
      "tests": { "description": "Build tests", "dependencies": ["boost-test"] }
    },
    "builtin-baseline": "5ee5eee0d3e9c6098b24d263e9099edcdcef6631",
    "vcpkg-configuration": {
        "overlay-triplets": ["./vcpkg-triplets"],
        "overlay-ports": [
          "vcpkg-ports/openssl",
          "vcpkg-ports/xmlsec"
        ]
    }
}
