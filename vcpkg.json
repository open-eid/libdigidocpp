{
    "name": "libdigidocpp",
    "dependencies": [
      {
        "name": "libxml2",
        "default-features": false,
        "platform": "!osx & !ios"
      },
      "xmlsec",
      {
        "name": "zlib",
        "platform": "!osx & !ios"
      }
    ],
    "features": {
      "tests": { "description": "Build tests", "dependencies": ["boost-test"] }
    },
    "builtin-baseline": "bc38a15b0bee8bc48a49ea267cc32fbb49aedfc4",
    "vcpkg-configuration": {
        "overlay-triplets": ["./vcpkg-triplets"],
        "overlay-ports": ["vcpkg-ports/libxml2", "vcpkg-ports/xmlsec"]
    }
}
